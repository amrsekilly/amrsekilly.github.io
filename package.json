{
  "private": true,
  "name": "site",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "prod": "mkdir public/src && mkdir public/src/images && gatsby build && cp CNAME public && cp robots.txt public && cp RefundPolicy.html public",
    "clean": "gatsby clean",
    "proxy": "netlify-cms-proxy-server",
    "deploy": "yarn prod && gh-pages -d public -b master"
  },
  "dependencies": {
    "@narative/gatsby-theme-novela": "^0.*",
    "gatsby": "^2.13.41",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.12",
    "gatsby-plugin-netlify-cms": "^4.1.40",
    "netlify-cms-app": "^2.11.23",
    "netlify-cms-proxy-server": "^1.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sharp": "^0.25.4"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}